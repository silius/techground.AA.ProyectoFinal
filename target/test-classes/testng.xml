<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Login Regression Suite">
    <groups>
        <run>
            <include name="login"/>
            <exclude name="broken"/>
        </run>
    </groups>
    <test name="allow_user_to_login">
        <parameter name="username" value="m@null.net"></parameter>
        <parameter name="password" value="test123"></parameter>
        <classes>
            <class name="loginTest"></class>
        </classes>
    </test>
</suite>